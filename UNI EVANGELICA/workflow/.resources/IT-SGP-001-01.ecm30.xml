    <WorkflowProcessEvent>
      <workflowProcessEventPK>
        <companyId>1</companyId>
        <eventId>afterTaskSave</eventId>
        <processId>IT-SGP-001-01</processId>
        <version>1</version>
      </workflowProcessEventPK>
      <eventDescription>function afterTaskSave(colleagueId,nextSequenceId,userList){&#xd;
	&#xd;
	var usuario = getUsuario(colleagueId);	&#xd;
	var horaCorrente = obterDataHoraCorrente();&#xd;
	&#xd;
	if ( nextSequenceId == 04 &amp;&amp; hAPI.getCardValue(&apos;reuniao&apos;) == &apos;..&apos; ) {&#xd;
    	addfilho(usuario, &quot;Solicitação salva como rascunho.&quot;, horaCorrente, &quot;Rascunho&quot;);&#xd;
    } else &#xd;
    if ( nextSequenceId == 37 ) {&#xd;
    	addfilho(usuario, hAPI.getCardValue(&apos;ataReuniao&apos;), horaCorrente, &quot;Reunião realizada&quot;);&#xd;
    } else&#xd;
    if ( nextSequenceId == 11 ) {&#xd;
    	addfilho(usuario, &quot;Solicitação de processo encaminhado para Análise.&quot;, horaCorrente, &quot;Encaminhada para Análise&quot;);&#xd;
    } else &#xd;
    if ( nextSequenceId == 17 ) {&#xd;
    	addfilho(usuario, hAPI.getCardValue(&apos;analiseSolicitacao&apos;), horaCorrente, &quot;Encaminhada para Desenvolvimento&quot;);&#xd;
    } else &#xd;
    if ( nextSequenceId == 26 &amp;&amp; hAPI.getCardValue(&apos;atividadeAtual&apos;) == &apos;11&apos; ) {&#xd;
    	addfilho(usuario, hAPI.getCardValue(&apos;analiseSolicitacao&apos;), horaCorrente, &quot;Reunião Solicitada&quot;);&#xd;
    } else &#xd;
    if ( nextSequenceId == 21 ) {&#xd;
    	addfilho(usuario, hAPI.getCardValue(&apos;analiseDesenvolvimento&apos;), horaCorrente, &quot;Desenvolvimento Concluído&quot;);&#xd;
    } else &#xd;
    if ( nextSequenceId == 07 ) {&#xd;
    	addfilho(usuario, &quot;Desenvolvimento de processo concluído e aprovado pelo solicitante.&quot;, horaCorrente, &quot;Desenvolvimento Concluído e Aprovado&quot;);&#xd;
    }else &#xd;
    if ( nextSequenceId == 26 &amp;&amp; hAPI.getCardValue(&apos;atividadeAtual&apos;) == &apos;21&apos;  ) {&#xd;
    	addfilho(usuario, hAPI.getCardValue(&apos;observacaoFinal&apos;), horaCorrente, &quot;Reunião Solicitada&quot;);&#xd;
    }&#xd;
	&#xd;
}&#xd;
&#xd;
function getUsuario(user) {&#xd;
&#xd;
	var c1 = DatasetFactory.createConstraint(&quot;colleaguePK.colleagueId&quot;, user,user, ConstraintType.MUST);&#xd;
	var datasetUser = DatasetFactory.getDataset(&quot;colleague&quot;, null, [ c1 ], null);&#xd;
	var colleague = &apos;&apos;;&#xd;
	if (datasetUser.rowsCount &gt; 0) {&#xd;
		colleague =  datasetUser.getValue(0, &quot;colleagueName&quot;);&#xd;
	}&#xd;
	return colleague;&#xd;
	&#xd;
}&#xd;
&#xd;
&#xd;
function addfilho(nome, descricao, horaCorrente, status) {	&#xd;
	&#xd;
    var childData = new java.util.HashMap();    &#xd;
    childData.put(&quot;histUsuario&quot;, nome);&#xd;
    childData.put(&quot;histData&quot;, horaCorrente);&#xd;
    childData.put(&quot;histInteracao&quot;, descricao);&#xd;
    childData.put(&quot;histStatus&quot;, status);    &#xd;
	hAPI.addCardChild(&apos;tabelaHistorico&apos;, childData);&#xd;
	&#xd;
}&#xd;
&#xd;
&#xd;
function obterDataHoraCorrente() {&#xd;
	&#xd;
	var today = new Date();&#xd;
	var year = today.getFullYear();&#xd;
	var month = (today.getMonth() + 1) &lt; 10 ? &apos;0&apos; + (today.getMonth() + 1)	: (today.getMonth() + 1);&#xd;
	var day = today.getDate() &lt; 10 ? &apos;0&apos; + today.getDate() : today.getDate();		&#xd;
	var hour = today.getHours() &lt; 10 ? &apos;0&apos; + today.getHours() : today.getHours();&#xd;
	var minute = today.getMinutes() &lt; 10 ? &apos;0&apos; + today.getMinutes() : today.getMinutes();&#xd;
	var second = today.getSeconds() &lt; 10 ? &apos;0&apos; + today.getSeconds() : today.getSeconds();&#xd;
	var currentHour = hour + &quot;:&quot; + minute + &quot;:&quot; + second;&#xd;
	var currentDate = day + &apos;/&apos; + month + &apos;/&apos; + year;&#xd;
	var currentTime = currentDate + &quot;  &quot; + currentHour;&#xd;
	&#xd;
	return currentTime;&#xd;
	&#xd;
}</eventDescription>
    </WorkflowProcessEvent>
